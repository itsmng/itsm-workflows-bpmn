h3 Instances

table
	tr
		td Name
		td Case Id
		td Started
		td Ended
		td Examine Log
		if user && user.isAdmin()
			td Delete
	each instance in instances
		- var ref = "/user/instanceDetails?id="+instance.id;
		if user && user.isAdmin()
			- var ref2 = "/user/deleteInstance?id="+instance.id;
		tr
			td #{instance.name}
			td #{instance.data.caseId}
			td #{instance.fromNow}
			td #{instance.endFromNow} 
			td
				a(href=`${ref}`) Examine
			if user && user.isAdmin()
				td 
					a(href=`${ref2}`) Delete
